{
    "name": "ibondoc/bigbluebutton",
    "description": "BigBlueButton Server API Library for Laravel",
    "keywords": [
        "bigbluebutton",
        "api",
        "bbb",
        "meeting",
        "package",
        "laravel-package",
        "laravel",
        "php"
    ],
    "homepage": "https://github.com/ibondoc/bigbluebutton",
    "license": "MIT",
    "type": "library",
    "require": {
        "php": ">=7.0",
        "illuminate/support": "^5.5|^6.0|^7.0|^8.0",
        "bigbluebutton/bigbluebutton-api-php": "^2.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "ibondoc\\Bigbluebutton\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "ibondoc\\Bigbluebutton\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "ibondoc\\Bigbluebutton\\BigbluebuttonServiceProvider"
            ],
            "aliases": {
                "Bigbluebutton": "ibondoc\\Bigbluebutton\\Facades\\Bigbluebutton"
            }
        }
    }
}
